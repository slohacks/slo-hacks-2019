    "npm": "^5.8.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-invisible-recaptcha": "^0.2.7",
    "react-helmet": "^5.2.0",
